#include<stdio.h>
int main() {
int n, key, i, found = 0;
printf("Enter number of elements:");
scanf("%d",n);

int arr[n];
printf(Enter elemnts: ");
for(i = 0; i<n; i++)
scanf("%d", &arr[i]);
printf("Enter element to search: ");
scanf("%d", &key);
for(i = 0; i< n; i++) {
if(arr[i] == key) {
printf("Element found at position %d\n", i + 1);
found = 1;
break;
}
}
if(!found)
printf(Element not found.\n");
return 0;


OUTPUT
Enter a number of Elements = 5
Enter Elements = 8 4 9 2 1
Enter Elements to search = 6
Element not found
